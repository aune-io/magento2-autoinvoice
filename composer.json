{
    "name": "aune-io/magento2-autoinvoice",
    "description": "Automatically invoice all complete orders in your Magento 2 store",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~7.0.13|~7.1.0",
        "magento/framework": "101.0.*",
        "magento/module-sales": "101.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2.0",
        "squizlabs/php_codesniffer": "3.0.1",
        "phpmd/phpmd": "@stable",
        "satooshi/php-coveralls": "~1.0.1"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aune\\AutoInvoice\\": ""
        }
    }
}
