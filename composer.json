{
    "name": "aune-io/magento2-autoinvoice",
    "description": "Automatically invoice all complete orders in your Magento 2 store",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~7.1.0|~7.2.0|~7.3.0|~7.4.0|~8.1.0",
        "magento/framework": "101.0.*|102.0.*|103.0.*",
        "magento/module-sales": "101.0.*|102.0.*|103.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.13",
        "squizlabs/php_codesniffer": "3.3.1",
        "phpmd/phpmd": "@stable",
        "php-coveralls/php-coveralls": "~2.1.0"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aune\\AutoInvoice\\": ""
        }
    }
}
